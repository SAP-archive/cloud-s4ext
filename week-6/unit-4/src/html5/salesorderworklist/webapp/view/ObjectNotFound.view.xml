<mvc:View
	controllerName="com.acme.s4ext.salesorderworklist.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		id="page"
		icon="{sap-icon://product}"
		description=""
		text="{i18n>noObjectFoundText}"
		title="{i18n>objectTitle}">
		<customDescription>
			<Link 
				id="link" 
				press="onLinkPressed"
				text="{i18n>backToWorklist}"/>
		</customDescription>
	</MessagePage>

</mvc:View>