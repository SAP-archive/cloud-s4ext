<mvc:View
	controllerName="com.acme.s4ext.salesorderworklist.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		id="page"
		icon="{sap-icon://document}"
		description=""
		text="{i18n>notFoundText}"
		title="{i18n>notFoundTitle}">
		<customDescription>
			<Link 
				id="link" 
				text="{i18n>backToWorklist}" 
				press="onLinkPressed" />
		</customDescription>
	</MessagePage>

</mvc:View>